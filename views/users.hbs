<h1>Users</h1>
<table>
  <thead>
    <tr>
      <th>id</th>
      <th>phone</th>
    </tr>
  </thead>
  <tbody>
    {{#each users}}
    <tr class="clickable" onclick="window.location='/admin/users/{{this.id}}';">
      <td>{{this.id}}</td>
      <td>{{this.phone}}</td>
    </tr>
    {{/each}}
  </tbody>
</table>